# a) Dar de alta un estudiante
POST http://localhost:8080/api/estudiantes
Content-Type: application/json
{
    "id": 10,
    "nombre": "Ana",
    "apellido": "Bellinzona",
    "edad": 21,
    "genero": "Femenino",
    "ciudad": "Buenos Aires",
    "documento": 46094519,
    "matriculas": [],
    "lu": 56789
}

# b) Matricular un estudiante en una carrera
POST http://localhost:8080/api/matriculas
Content-Type: application/json
{
    "id": 1,
    "estudianteId": 1,
    "carreraId": 1,
    "inscripcion": 2020,
    "graduacion": 0,
    "antiguedad": 5
}

# c) Recuperar todos los estudiantes (ordenados por edad)
GET http://localhost:8080/api/estudiantes/ordenadosEdad

# c) Recuperar todos los estudiantes (sin ordenamiento)
GET http://localhost:8080/api/estudiantes

# d) Recuperar un estudiante por número de libreta universitaria
GET http://localhost:8080/api/estudiantes/nroLibreta/72976

# e) Recuperar todos los estudiantes por género
GET http://localhost:8080/api/estudiantes/genero/femenino

# f) Recuperar carreras con estudiantes inscriptos (ordenadas por cantidad)
GET http://localhost:8080/api/carreras/activas

# g) Recuperar estudiantes de una carrera filtrados por ciudad
GET http://localhost:8080/api/estudiantes/carrera/abogacia/ciudad/tandil

# h) Generar reporte de carreras con inscriptos y egresados por año
GET http://localhost:8080/api/carreras/reporte

# Buscar estudiantes por edad
GET http://localhost:8080/api/estudiantes/edad/20